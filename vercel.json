{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "backend/backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "backend/backend/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/backend/wsgi.py"
    }
  ]
}